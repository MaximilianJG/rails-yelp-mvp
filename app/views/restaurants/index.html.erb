<div class="text-align-center">


<h1>All Restaurants</h1>

<ul>
  <% @restaurants.each do |restaurant| %>
    <li class="card p-5"><%= link_to restaurant.name, restaurant_path(restaurant) %></li>
    <br>
  <% end %>
</ul>

<%= link_to "Create a new restaurant", new_restaurant_path, class: "btn btn-primary" %>

</div>

